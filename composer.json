{
  "name": "theulem/website",
  "description": "ULEM  Website",
  "homepage": "https://ulem.org",
  "authors": [
    {
      "name": "Dele Tosh",
      "email": "ask@deletosh.com",
      "homepage": "https://github.com/deletosh"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    },
    {
      "type": "vcs",
      "url": "git@github.com:authwit/wp-plugin-betterlinks-pro.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:authwit/wp-plugin-flying-press.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:authwit/wp-plugin-seo-by-rank-math-pro.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:authwit/wp-plugin-fluentformpro.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:authwit/wp-plugin-advanced-custom-fields-pro.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:authwit/wp-plugin-kadence-pro.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:authwit/wp-plugin-kadence-blocks-pro.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:authwit/wp-plugin-advanced-custom-fields-pro.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:authwit/deployer-wp-recipes.git"
    }
  ],
  "require": {
    "authwit/betterlinks-pro": "*",
    "authwit/flying-press": "*",
    "authwit/advanced-custom-fields-pro": "*",
    "authwit/fluentformpro": "*",
    "authwit/kadence-blocks-pro": "*",
    "authwit/kadence-pro": "*",
    "authwit/seo-by-rank-math-pro": "*",
    "authwit/deployer-wp-recipes": "*",
    "composer/installers": "^2.2",
    "oscarotero/env": "^2.1",
    "php": ">=8.0",
    "roots/bedrock-autoloader": "^1.0",
    "roots/bedrock-disallow-indexing": "^2.0",
    "roots/wordpress": "^6",
    "roots/wp-config": "1.0.0",
    "roots/wp-password-bcrypt": "1.1.0",
    "vlucas/phpdotenv": "^5.5",
    "wpackagist-plugin/betterlinks": "*",
    "wpackagist-plugin/fluentform": "*",
    "wpackagist-plugin/kadence-blocks": "*",
    "wpackagist-plugin/kadence-starter-templates": "*",
    "wpackagist-plugin/seo-by-rank-math": "*",
    "wpackagist-theme/kadence": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7.1",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "composer/installers": true,
      "roots/wordpress-core-installer": true
    },
    "github-oauth": {
      "github.com": "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"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "web/app/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "web/app/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "test": [
      "phpcs"
    ]
  }
}
